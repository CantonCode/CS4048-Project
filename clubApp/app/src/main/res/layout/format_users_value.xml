<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:padding="20dp">


    <TextView
        android:id="@+id/list_photo"
        android:layout_width="120dp"
        android:layout_height="30dp"
        android:layout_marginStart="88dp"
        android:text="TextView"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/list_studentNumber"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.447" />

    <TextView
        android:id="@+id/list_admin"
        android:layout_width="120dp"
        android:layout_height="30dp"
        android:layout_marginStart="17dp"
        android:text="TextView"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/list_studentNumber"
        android:layout_width="120dp"
        android:layout_height="30dp"
        android:layout_marginStart="17dp"
        android:layout_marginTop="28dp"
        android:layout_marginBottom="28dp"
        android:text="TextView"
        app:layout_constraintBottom_toTopOf="@+id/list_userID"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/list_admin" />

    <TextView
        android:id="@+id/list_userID"
        android:layout_width="120dp"
        android:layout_height="30dp"
        android:layout_marginStart="17dp"
        android:layout_marginTop="28dp"
        android:text="TextView"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/list_studentNumber" />

    <TextView
        android:id="@+id/list_userName"
        android:layout_width="120dp"
        android:layout_height="30dp"
        android:layout_marginStart="17dp"
        android:layout_marginTop="24dp"
        android:text="TextView"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/list_userID" />
    " />
</androidx.constraintlayout.widget.ConstraintLayout>